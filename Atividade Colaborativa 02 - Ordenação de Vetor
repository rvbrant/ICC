#include <stdio.h>
#include <stdlib.h>


char vogais[5];
int i;

void Ordenar(char vogais[], int i, int j){
  char aux;

  if(i==3){
    for(i=0; i<5; i++){
        printf("%c", vogais[i]);
    }
  }
  else{

    if(j==5){
        Ordenar(vogais, i+1, i+2);
    }

    else{
        if(vogais[i]>vogais[j]){
            aux=vogais[i];
            vogais[i]=vogais[j];
            vogais[j]=aux;
        }
            Ordenar(vogais, i, j+1);
    }
  }
}

int main(){
  for(i=0; i<5; i++){
    scanf("%c", &vogais[i]);
  }
  Ordenar(vogais, 0, 1);
  return 0;
}
