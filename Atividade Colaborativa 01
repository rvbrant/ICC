#include<stdio.h>

int main() {
    float peso, altura;
    char sexo;
    printf("Digite o seu sexo");
    scanf("%c", &sexo);
    if(sexo == 'masculino');{
        printf("Digite sua altura");
        scanf("%f", &altura, &peso);{
            peso = (72.7 * altura) - 58;
            printf("peso");
        }
        
    }
        
}
