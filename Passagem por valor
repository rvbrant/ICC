#include <stdio.h>
#include <stdlib.h>

void swap(int a, int b){
    int temp;
    temp = b;
    b = a;
    a = temp;
}

int main(){
    int a,b;
    printf("Entre c/ valores:");
    scanf("%d %d", &a,&b);
    printf("a=%d b=%d \n", a,b);

    swap(a,b);

    printf("a=%d b=%d \n", a,b);
    return 0;
}
